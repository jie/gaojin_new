@import "../../libs/weui/weui.wxss";
@import "../../assets/style.wxss";


.view-page {
  min-height: 100vh;
  font-size: 26rpx;
  padding-bottom: 30px;
}

.place-description {
  padding-left: 15px;
  padding-right: 10px;
  padding-bottom: 10px;
  color: #666;
}

.view_info_wrap {
  padding-bottom: 0!important;
}

.view_num, .view_count {
  display: flex;
  align-items: center;
}

.view_num image, .view_count image {
  margin-right: 5px;
}

.wxParse-p {
  margin-bottom: 10px;
}

.wxParse-p:last-child {
  margin-bottom: 0
}

.content {
  padding: 10px 15px;
  color: #666;
  text-align: justify;
}

.empty-cell {
  height: 3rpx;
  border-bottom: 1px solid #f5f5f5;
  margin-left: 15px;
}

