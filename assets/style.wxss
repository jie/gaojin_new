/*btn*/
.weui-btn{
    font-size: 15px;
    line-height: 3.2;
}
.weui-btn_primary{
    background-color: #b6a37e;
}
.weui-btn_disabled.weui-btn_primary {
    background-color: #b6a37e;
    opacity: .5;
}
.weui-btn_primary:not(.weui-btn_disabled):active {
    color: hsla(0,0%,100%,.8);
    background-color: #b6a37e;
    opacity: .8;
}
.weui-btn_plain-primary {
    color: #b6a37e;
    border: 1px solid #b6a37e;
}
.weui-btn_plain-default {
    color: #555;
    border: 1px solid #bbb;
}
.weui-btn.weui-btn_mini {
    font-size: 14px;
    line-height: 2.2;
    padding: 0 8px;
    border-radius: 5px;
}

/*weui-media-box*/
.weui-panel__hd{
    color: #333;
    font-size: 16px;
}
.weui-panel__hd:after{
    left: 0;
}
.weui-panel__hd .num{
    float: right;
    color: #999;
    font-size: 14px;
}
.weui-panel .weui-media-box__title-after{
    font-size: 12px;
}
.weui-media-box{
    position: relative;
}
.weui-media-box.weui-media-small{
    padding: 10px 15px;
}
.weui-media-box.weui-media-small .weui-media-box__title{
    font-size: 15px;
}
a .weui-media-box__title{
    color: #333;
}
.weui-media-box.weui-media-small .weui-media-box__hd{
    width: 40px;
    height: 40px;
    line-height: 40px;
    border-radius: 50%;
    overflow: hidden;
    position: relative;
}
.weui-media-box.weui-media-small .weui-media-box__hd.icon-box{
    border-radius: 5px;
}
.weui-media-box.weui-media-small .weui-media-box__hd .iconfont{
    font-size: 20px;
}
.weui-media-box .weui-media-box__desc {
    font-size: 12px;
    margin-top: 5px;
}
.weui-media-box.weui-media-small .weui-media-box__desc{
