/*filter-list*/
.filter-top{
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    z-index: 999;
}
.filter-top .weui-cells:first-child{
    margin-top: 0;
    overflow: visible;
}
.filter-top .weui-cells:first-child:before{
    border-top: 0;
}
.filter-top{
    font-size: 12px;
}
.filter-top .weui-label{
    width: 20px;
}
.filter-top .iconfont{
    color: #000;
    font-size: 12px;
}
.filter-top .weui-cell:before{
    border-top: 0;
}
.filter-top .weui-cell{
    padding: 0 15px;
}
.filter-top ul{
    padding-left: 0;
}
.filter-top ul.category{
    display: table;
    width: 100%;
}
.filter-top ul.category>li{
    display: table-cell;
    text-align: center;
    font-size: 14px;
}
.filter-top ul.category>li:first-child{
    text-align: left;
}
.filter-top ul.category>li:last-child{
    text-align: right;
}
.filter-top ul.category .dropdown-toggle{
    display: inline-block;
    padding: 15px 0;
}
.filter-top ul.category>li .iconfont{
    font-size: 10px;
    display: inline-block;
    margin-left: 5px;
}
.filter-top ul.dropdown{
    position: absolute;
    left: 0;
    right: 0;
    top: 100%;
    background: #fff;
    z-index: 999;
    max-height: 240px;
    overflow-y: auto;
    display: none;
}
.filter-top ul.dropdown.show{
    display: block;
}
.filter-top ul.dropdown li{
    padding: 10px 15px;
    position: relative;
    text-align: left!important;
    font-size: 12px;
    list-style: none;
}
.filter-top ul.dropdown li a,.filter-top ul.dropdown li a:visited{
    color: #666;
}
.filter-top ul.dropdown li:after{
    content: " ";
    position: absolute;
    left: 0;
    right: 0;
    height: 1px;
    bottom: 0;
    color: #e5e5e5;
    z-index: 2;
    border-bottom: 1px solid #e5e5e5;
    -webkit-transform-origin: 0 100%;
    transform-origin: 0 100%;
    -webkit-transform: scaleY(.5);
    transform: scaleY(.5);
}
.filter-container .shadow{
    position: absolute;
    top: 0;
    bottom: 56px;
    left: 0;
    right: 0;
    background: rgba(0,0,0,.5);
    z-index: 998;
    display: none;
}
.filter-container .shadow.show{
    display: block;
}
.filter-content{
    margin-top: 49px;
}
/*filter END*/


