/*register*/
.register-process{
    padding: 20px 15px;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    -ms-align-items: center;
    align-items: center;
    position: relative;
    background: #f5f5f5;
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    z-index: 99;
    color: #333;
}
.register-process:before{
    content: "";
    position: absolute;
    top: 35px;
    height: 2px;
    left: 46px;
    right: 46px;
    background: #aaa;
    z-index: 0;
}
.register-process a{
    color: #333;
}
.register-process .process-item{
    position: relative;
    z-index: 2;
    text-align: center;
    font-size: 14px;
}
.register-process .process-item .step{
    width: 32px;
    height: 32px;
    background: #aaa;
    color: #fff;
    font-size: 20px;
    line-height: 32px;
    text-align: center;
    border-radius: 50%;
    margin: 0 auto;
    margin-bottom: 5px
}
.register-process .process-item.active .step{
    background: #b6a37e;
}
.register-process .process-item.active .label{
    color: #b6a37e;
}
.register-process + div{
    margin-top: 99px;
    color: #555;
}
/*register END*/


