.tips:before{
    position: absolute;
    content: "";
    border-top: 1px solid #ddd;
    top: 50%;
    margin-top: -1px;
    left: 15px;
    right: 15px;
    z-index: 1;
}
.tips span{
    display: inline-block;
    padding: 5px 10px;
    background: #f4f4f4;
